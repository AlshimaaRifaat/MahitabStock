<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowDisablePreview">true
        </item> <!--Disable dummy starting window of application -->

      <!-- target android sdk versions < 26 and > 14 if theme other than AppCompat -->
    </style>

    <style name="ProgressDialog" parent="Theme.AppCompat.Light.Dialog.Alert">
        <item name="android:textColor">@android:color/black</item>
        <item name="colorAccent">@color/colorPrimary</item>
        <item name="android:background">@android:color/white</item>
    </style>



</resources>